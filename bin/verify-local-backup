#!/usr/bin/env bash

local dest="/mnt/backup-internal/willhaley-backup"

wrsync --delete-excluded --delete-after --checksum --dry-run "${HOME}/Photographs/" "${dest}/Photographs/"
wrsync --delete-excluded --delete-after --checksum --dry-run "${HOME}/Music/"       "${dest}/Music/"
wrsync --delete-excluded --delete-after --checksum --dry-run "${HOME}/Documents/"   "${dest}/Documents/"
